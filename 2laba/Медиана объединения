import java.util.ArrayList;
import java.util.Arrays;
import java.util.List;
import java.util.Scanner;

public class LeftMedian {
    public static void main(String[] args) {
        Scanner scanner = new Scanner(System.in);
        int N = scanner.nextInt();
        int L = scanner.nextInt();
        List<int[]> list = new ArrayList<>();
        for (int i = 0; i < N; i++) {
            int[] line = new int[L];
            for (int j = 0; j < L; j++) {
                line[j] = scanner.nextInt();
            }
            list.add(line);
        }
        StringBuilder result = new StringBuilder();
        for (int i = 0; i < N; i++) {
            for (int j = i + 1; j < N; j++) {
                int[] merger = mergerAndSort(list.get(i), list.get(j));
                result.append(merger[L - 1]).append("\n");
            }
        }
        System.out.print(result);
    }

    private static int[] mergerAndSort(int[] seq1, int[] seq2) {
        int[] merger = new int[seq1.length + seq2.length];
        System.arraycopy(seq1, 0, merger, 0, seq1.length);
        System.arraycopy(seq2, 0, merger, seq1.length, seq2.length);
        Arrays.sort(merger);
        return merger;
    }
}
